<?xml version="1.0"?>
<!-- 
  The only override for the Pages/Public, Style, Images, and Security folders is that no authentication is required.
-->
<configuration xmlns="http://schemas.microsoft.com/.NetConfiguration/v2.0">
  <system.web>
    <httpRuntime requestPathInvalidCharacters="" requestValidationMode="2.0" />
    <pages validateRequest="false" />
    <authorization>
      <allow users="*" />
    </authorization>
  </system.web>
</configuration>
